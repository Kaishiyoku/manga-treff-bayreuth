{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "albertcht/invisible-recaptcha": "1.8.4",
        "ezyang/htmlpurifier": "4.10.0",
        "fideloper/proxy": "4.0.0",
        "guzzlehttp/guzzle": "6.3.3",
        "judev/php-htmltruncator": "1.1.0",
        "kaishiyoku/animexx-api": "^0.1.0",
        "kaishiyoku/laravel-menu": "2.0.0",
        "laracasts/flash": "3.0.2",
        "laravel/framework": "5.7.13",
        "laravel/tinker": "1.0.8",
        "laravelcollective/html": "5.7.1",
        "spatie/laravel-google-calendar": "2.2.1",
        "symfony/dom-crawler": "4.1.7",
        "torann/geoip": "1.0.8",
        "webup/laravel-sendinblue": "1.1.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.5.2",
        "doctrine/dbal": "2.8.0",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "2.1.0",
        "phpunit/phpunit": "7.4.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir=\"app/Models\" --write"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
